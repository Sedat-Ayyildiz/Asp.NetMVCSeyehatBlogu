@using TravelBlog.Models.Siniflar
@model List<Blog>

<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <a href="/Blog/BlogDetay/@x.ID"><img src="@x.BLOGIMAGE" alt="" /></a>
            </div>
            <div class="bot-desc">
                <p>@x.ACIKLAMA.Substring(0,75) ...</p>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5","Default")
</div>
